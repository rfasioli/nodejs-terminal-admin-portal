<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
    
    <script src="/controllers/reports/usuario_cofre.js"></script>
    <script src="/dist/js/angular-locale_pt-br.js"></script>
  </head>
  
  <body class="hold-transition skin-orion sidebar-mini">
    <div class="wrapper">
      
      <!-- PAGE HEADER -->
      <% include ../partials/header %>
      
      <!-- SIDE BAR MENU -->
      <% include ../partials/menu %>
      
      <!-- PAGE CONTENT -->
      <!-- PAGE CONTENT -->
      <!-- PAGE CONTENT -->
      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Usuários do Cofre
            <small>Relatórios</small><!DOCTYPE html>
          </h1>
          <ol class="breadcrumb">
            <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
            <li>Relatórios</li>
            <li class="active">Usuários do Cofre</li>
          </ol>
        </section>
        
        <!-- Main content -->
        <section class="content">
          <div class="table-responsive" ng-app="usuarioCofreApp" ng-controller="usuarioCofreController">
            <table class="table table-condensed table-striped table-bordered table-hover no-margin">
              <thead>
                <tr>
                  <th style="align:center">Nº Terminal</th>
                  <th style="align:center">ID Terminal</th>
                  <th style="align:center">Identificador</th>
                  <th style="align:center">Nome</th>
                  <th style="align:center">Ativo</th>
                  <th style="align:center">Perfil</th>
                  <th style="align:center">Biometria</th>
                </tr>
              </thead>
              <tbody>
                <tr ng-repeat="usuario in usuarios">
                  <td><center>{{usuario.NR_TERMINAL}}</center></td>
                  <td><center>{{usuario.ID_TERMINAL}}</center></td>
                  <td><center>{{usuario.ID_IDENTIFICADOR}}</center></td>
                  <td>{{usuario.NM_USUARIO}}</td>
                  <td><center>{{usuario.IC_ATIVO}}</center></td>
                  <td>{{usuario.DS_PERFIL}}</td>
                  <td><center>{{usuario.DS_BIOMETRIA}}</center></td>
                </tr>
              </tbody>
            </table>
          </div>
        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
      
      
      <!-- PAGE FOOTER -->
      <% include ../partials/footer %>
      
      <!-- CONTROLS -->
      <% include ../partials/controls %>
      
    </div>
  </body>
</html>
